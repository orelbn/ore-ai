{
	"name": "ore-ai-web",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite dev --port 3000",
		"build": "vite build && tsgo --noEmit",
		"preview": "vite preview --port 3000",
		"typecheck": "tsgo --noEmit",
		"lint": "biome check .",
		"format": "biome format --write .",
		"test": "bun test",
		"deploy": "bun run build && wrangler deploy",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts",
		"auth:generate": "bunx @better-auth/cli generate --output src/db/schema/auth.ts -y",
		"db:generate": "bunx drizzle-kit generate",
		"db:migrate:local": "wrangler d1 migrations apply ore-ai --local",
		"db:migrate:prod": "wrangler d1 migrations apply ore-ai --remote"
	},
	"dependencies": {
		"@ai-sdk/react": "^3.0.100",
		"@base-ui/react": "^1.2.0",
		"@fontsource-variable/geist": "^5.2.8",
		"@hugeicons/core-free-icons": "^3.1.1",
		"@hugeicons/react": "^1.1.5",
		"@tanstack/react-router": "^1.132.0",
		"@tanstack/react-start": "^1.132.0",
		"@tanstack/router-plugin": "^1.132.0",
		"ai": "^6.0.98",
		"better-auth": "^1.4.19",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"drizzle-orm": "^0.45.1",
		"react": "^19.2.4",
		"react-dom": "^19.2.4",
		"tailwind-merge": "^3.5.0",
		"workers-ai-provider": "^3.1.2"
	},
	"devDependencies": {
		"@typescript/native-preview": "^7.0.0-dev.20260225.1",
		"@biomejs/biome": "2.4.4",
		"@cloudflare/vite-plugin": "^1.13.8",
		"@tailwindcss/vite": "^4.2.1",
		"@types/bun": "^1.3.9",
		"@types/node": "^25.3.0",
		"@types/react": "^19.2.14",
		"@types/react-dom": "^19.2.3",
		"@vitejs/plugin-react": "^5.0.4",
		"babel-plugin-react-compiler": "^1.0.0",
		"drizzle-kit": "^0.31.9",
		"shadcn": "^3.8.5",
		"tailwindcss": "^4.2.1",
		"tw-animate-css": "^1.4.0",
		"vite": "^7.1.7",
		"vite-tsconfig-paths": "^5.1.4",
		"wrangler": "^4.68.0"
	}
}
